    <CheatEntry>
      <ID>6</ID>
      <Description>"根据名字获取窗口客户区坐标 x86"</Description>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>[ENABLE]

// 搜索的窗口名字
alloc(form_name,$100)
form_name:
dw '迷你世界',0
registersymbol(form_name)

// 输出到这里
alloc(form_xy,$10)
form_xy:
dd 0,0,0,0
registersymbol(form_xy)

// 写入代码
alloc(form_code,$1000)
createthread(form_code)

form_code:

  push ebp
  mov ebp,esp
  sub esp,18
  push esi
  push edi

label0:

  push form_name
  push 00
  call FindWindowW
  mov esi,eax

  lea edi,[ebp-18]
  xor eax,eax
  stosd
  stosd
  stosd
  stosd

  lea eax,[ebp-18]
  push eax
  push esi
  call GetClientRect

  lea eax,[ebp-08]
  mov [ebp-08],00000000
  mov [ebp-04],00000000
  push eax
  push esi
  call ClientToScreen

  mov eax,[ebp-08]
  mov [form_xy+00],eax
  mov eax,[ebp-04]
  mov [form_xy+04],eax
  mov eax,[ebp-10]
  sub eax,[ebp-18]
  mov [form_xy+08],eax
  mov eax,[ebp-0C]
  sub eax,[ebp-14]
  mov [form_xy+0C],eax

  push #100
  call Sleep

// 将这里替换为结束
label2:

  jmp label0

[DISABLE]

// 替换为结束
label2:

  xor eax,eax
  pop edi
  pop esi
  leave
  ret

unregistersymbol(*)
</AssemblerScript>
