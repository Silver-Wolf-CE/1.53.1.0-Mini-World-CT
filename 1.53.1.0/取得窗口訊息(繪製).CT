    <CheatEntry>
      <ID>1807637745</ID>
      <Description>"根据名字获取窗口客户区坐标"</Description>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>[ENABLE]

// 搜索的窗口名字
alloc(form_name,$100)
form_name:
dw 'QQ',0
registersymbol(form_name)

// 输出到这里
alloc(form_xy,$10)
form_xy:
dd 0,0,0,0
registersymbol(form_xy)

// 写入代码
alloc(form_code,$1000)
createthread(form_code)

form_code:

  push rbx
  sub rsp,30

label0:

  mov rdx,form_name
  xor ecx,ecx
  call FindWindowW
  mov rbx,rax

  test rax,rax
  je label1

  lea rdx,[rsp+20]
  mov rcx,rbx
  call GetClientRect

  lea rdx,[rsp+40]
  mov qword ptr [rsp+40],00000000
  mov rcx,rbx
  call ClientToScreen

  mov eax,[rsp+40]
  mov [form_xy+00],eax
  mov eax,[rsp+44]
  mov [form_xy+04],eax
  mov eax,[rsp+28]
  mov [form_xy+08],eax
  mov eax,[rsp+2C]
  mov [form_xy+0C],eax

label1:

  mov rcx,#100
  call Sleep

// 将这里替换为结束
label2:

  jmp label0

[DISABLE]

// 替换为结束
label2:

  add rsp,30
  pop rbx
  ret

unregistersymbol(*)
</AssemblerScript>
